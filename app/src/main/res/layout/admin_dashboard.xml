<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear2"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="3dp"
		android:padding="8dp"
		android:background="#171717"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/forelayout"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="#171717"
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/image_background"
				android:layout_width="100dp"
				android:layout_height="match_parent"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/user_profile_url"
					android:layout_width="80dp"
					android:layout_height="80dp"
					android:background="@android:color/transparent"
					android:src="@drawable/shopping_cart"
					android:scaleType="fitXY"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear5"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<TextView
					android:id="@+id/username"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:text="Username"
					android:textSize="16sp"
					android:textColor="#FFFFFF"/>
				<TextView
					android:id="@+id/mobile_number"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:text="Mob num "
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#FFFFFF"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear8"
				android:layout_width="30dp"
				android:layout_height="match_parent"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/swipe_dot_left"
					android:layout_width="3dp"
					android:layout_height="20dp"
					android:padding="8dp"
					android:background="#212121"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/swipe_dot_right"
					android:layout_width="3dp"
					android:layout_height="25dp"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:background="#212121"
					android:orientation="vertical"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/backlayout"
			android:layout_width="116dp"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/viewpager"
		android:layout_width="match_parent"
		android:layout_height="175dp"
		android:background="#455A64"
		android:orientation="horizontal"/>
	<LinearLayout
		android:id="@+id/linear9"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear13"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:orientation="horizontal"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/add_new_product"
				android:layout_width="75dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:padding="8dp"
				android:background="#00BCD4"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview4"
					android:layout_width="60dp"
					android:layout_height="60dp"
					android:src="@drawable/sekkpng"
					android:scaleType="fitCenter"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/textview1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="New Products "
					android:textSize="14sp"
					android:textColor="#000000"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/change_price"
				android:layout_width="75dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:padding="8dp"
				android:background="#BCAAA4"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview6"
					android:layout_width="60dp"
					android:layout_height="60dp"
					android:src="@drawable/credit_card"
					android:scaleType="fitCenter"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/textview3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Change prices"
					android:textSize="14sp"
					android:textColor="#000000"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/confirm_orders"
				android:layout_width="75dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:padding="8dp"
				android:background="#A5D6A7"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview5"
					android:layout_width="60dp"
					android:layout_height="60dp"
					android:src="@drawable/shopping"
					android:scaleType="fitCenter"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/textview2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Confirm Orders"
					android:textSize="14sp"
					android:textColor="#000000"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear29"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:orientation="horizontal"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/mail"
				android:layout_width="75dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:padding="8dp"
				android:background="#9FA8DA"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview8"
					android:layout_width="60dp"
					android:layout_height="60dp"
					android:src="@drawable/messages"
					android:scaleType="fitCenter"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/textview5"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Chat"
					android:textSize="14sp"
					android:textColor="#000000"
					android:layout_weight="1"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/share"
				android:layout_width="75dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:padding="8dp"
				android:background="#BDBDBD"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview9"
					android:layout_width="60dp"
					android:layout_height="wrap_content"
					android:src="@drawable/drive"
					android:scaleType="fitXY"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/textview6"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Upload to Drive "
					android:textSize="14sp"
					android:textColor="#000000"
					android:layout_weight="1"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/orders_trans"
				android:layout_width="75dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:padding="8dp"
				android:background="#FFF59D"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview7"
					android:layout_width="60dp"
					android:layout_height="60dp"
					android:src="@drawable/kindpng_337408"
					android:scaleType="fitCenter"
					android:layout_gravity="center_horizontal|center_vertical"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/textview4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Add Offers"
					android:textSize="14sp"
					android:textColor="#000000"
					android:layout_weight="1"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear33"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:gravity="center_horizontal|bottom"
			android:orientation="horizontal"
			android:layout_weight="4">
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:src="@drawable/account_card_details"
				android:scaleType="fitCenter"/>
			<ImageView
				android:id="@+id/imageview3"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:src="@drawable/logout"
				android:scaleType="fitCenter"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
